//Binary Search

#include<bits/stdc++.h>
using namespace std;
#define  ll long long

int main()
{   
        ll n, m, k, i, j, x, y, pos=0, s=0, h, f=0;
        cin>>n;
        ll a[n];
        for(i=0; i<n; i++){
        	cin>>a[i];
        }
        cin>>x;
        
        ll left=0, right = n-1, mid;
        while(left<=right){
        	mid = (left+right)/2;
        	if(a[mid]==x){
        		f=1;
        		pos = mid;
        		break;
        	}
        	else if(a[mid]>x){
        		right = mid - 1;
        	}
        	else{
        		left = mid + 1;
        	}
        }

        if(binary_search(a, a+n, x)){
            cout<<"Found"<<endl;
        }
        else {
            cout<<"Not found"<<endl;
        }

        if(f==1){
        	cout<<"Found in "<<pos<<" position."<<endl;
        }
        else{
        	cout<<"Not found."<<endl;
        }
      
    return 0;
}
