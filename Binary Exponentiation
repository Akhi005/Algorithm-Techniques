#include<bits/stdc++.h>
using namespace std;
#define ll long long
ll binary_exponentiation(ll a, ll b)
{
    ll res=1;
    while(b)
    {
        if(b&1)
            res=res*a;
        
        a*=a;
        b>>=1; //b=b/2 
    }
    return res;
}
int main()
{
    ll i,j,t,n,m,a,b,c,ans,k;
    cin>>a>>b;
    cout<<binary_exponentiation(a,b)<<endl;
}
